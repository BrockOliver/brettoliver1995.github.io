{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "",
  "data": { "url": "data/elk_RvN_hunt_days.csv"},
  "mark": "bar",
  "width": {"step": 17},
  "encoding": {
    "y": {
      "aggregate": "sum", "field": "days",
      "title": "Percentage of Population",
      "stack":  "normalize"
    },
    "x": {"field": "year"},
    "color": {
      "field": "residency",
      "scale": {"range": ["#675193", "#ca8861"]}
    }
  }
}